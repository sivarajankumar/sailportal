	<bean id="lostPasswordStudentReminderWizardController"
		class="org.telscenter.sail.webapp.presentation.web.controllers.LostPasswordStudentReminderWizardController">
		<property name="userService" ref="userService" />
		<property name="commandName" value="reminderParameters" />
		<property name="commandClass"
			value="org.telscenter.sail.webapp.domain.impl.ReminderParameters" />
	</bean>

	<bean id="reminderParameters"
		class="org.telscenter.sail.webapp.domain.impl.ReminderParameters"
		scope="prototype" />

	<bean id="runParameters"
		class="org.telscenter.sail.webapp.domain.impl.RunParameters"
		scope="prototype" />

	<bean id="lostPasswordMainController"
		class="org.telscenter.sail.webapp.presentation.web.controllers.LostPasswordMainController" />

	<bean id="lostPasswordTeacherMainController"
		class="org.telscenter.sail.webapp.presentation.web.controllers.LostPasswordTeacherMainController">
		<property name="userService" ref="userService" />
		<property name="commandName" value="userDetails" />
		<property name="commandClass"
			value="org.telscenter.sail.webapp.domain.authentication.impl.TeacherUserDetails" />
		<property name="validator" ref="lostTeacherValidator" />
		<property name="formView" value="lostpasswordteachermain" />
		<property name="successView" value="lostpasswordteacherresult" />
		<property name="javaMail" ref="javaMail"/>
	</bean>

	<bean id="lostPasswordTeacherErrorController"
		class="org.telscenter.sail.webapp.presentation.web.controllers.LostPasswordTeacherErrorController" />

	<bean id="lostPasswordStudentMainController"
		class="org.telscenter.sail.webapp.presentation.web.controllers.LostPasswordStudentMainController" />

	<bean id="lostPasswordStudentPasswordController"
		class="org.telscenter.sail.webapp.presentation.web.controllers.LostPasswordStudentPasswordController" />

	<bean id="lostPasswordStudentUsernameController"
		class="org.telscenter.sail.webapp.presentation.web.controllers.LostPasswordStudentUsernameController" />

	<bean id="lostPasswordTeacherResultController"
		class="org.telscenter.sail.webapp.presentation.web.controllers.LostPasswordTeacherResultController" />

	<bean id="lostPasswordStudentProjectCodeController"
		class="org.telscenter.sail.webapp.presentation.web.controllers.LostPasswordStudentProjectCodeController">
		<property name="runService" ref="runService" />
		<property name="commandName" value="reminderParameters" />
		<property name="validator" ref="lostPasswordStudentProjectCodeValidator"/>
		<property name="commandClass"
			value="org.telscenter.sail.webapp.domain.impl.ReminderParameters" />
		<property name="formView"
			value="lostpasswordstudentprojectcode" />
		<property name="successView"
			value="lostpasswordstudentprojectcoderesult" />
	</bean>

	<bean id="enterProjectCodeController"
		class="org.telscenter.sail.webapp.presentation.web.controllers.EnterProjectCodeController" />

	<bean id="studentArchivedProjectsController"
		class="org.telscenter.sail.webapp.presentation.web.controllers.StudentArchivedProjectsController" />

	<bean id="wiseOverviewController"
		class="org.telscenter.sail.webapp.presentation.web.controllers.WiseOverviewController" />

</beans>