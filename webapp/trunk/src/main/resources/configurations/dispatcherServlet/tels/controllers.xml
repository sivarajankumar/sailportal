<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.0.xsd">

	<!-- Custom controllers -->

	<bean id="studentUserDetailsValidator"
		class="org.telscenter.sail.webapp.presentation.validators.StudentUserDetailsValidator" />

	<bean id="teacherUserDetailsValidator"
		class="org.telscenter.sail.webapp.presentation.validators.TeacherUserDetailsValidator" />
		
	<bean id="addprojectparametersValidator" 
	    class="org.telscenter.sail.webapp.presentation.validators.student.AddProjectParametersValidator" />
	    
	<bean id="teamSignInFormValidator" 
	    class="org.telscenter.sail.webapp.presentation.validators.student.TeamSignInFormValidator" >
	    <property name="userService" ref="userService" />
	</bean>

	<bean id="lostTeacherValidator"
		class="org.telscenter.sail.webapp.presentation.validators.LostTeacherValidator" />

	<bean id="lostPasswordStudentUsernameValidator"
		class="org.telscenter.sail.webapp.presentation.validators.LostPasswordStudentUsernameValidator" />

	<bean id="lostPasswordStudentAccountQuestionValidator"
		class="org.telscenter.sail.webapp.presentation.validators.LostPasswordStudentAccountQuestionValidator" />

	<bean id="lostPasswordStudentProjectCodeValidator"
		class="org.telscenter.sail.webapp.presentation.validators.LostPasswordStudentProjectCodeValidator" />

	<bean
		class="org.telscenter.sail.webapp.presentation.web.controllers.TestController" />

	<bean id="teacherIndexController"
		class="org.telscenter.sail.webapp.presentation.web.controllers.TeacherIndexController" />

	<bean id="registerController"
		class="org.telscenter.sail.webapp.presentation.web.controllers.RegisterController" />

	<bean id="countriesController"
		class="org.telscenter.sail.webapp.presentation.web.controllers.CountriesController" />

	<bean id="statesController"
		class="org.telscenter.sail.webapp.presentation.web.controllers.StatesController" />

	<bean id="helpController"
		class="org.telscenter.sail.webapp.presentation.web.controllers.HelpController" />

	<bean id="indexController"
		class="org.telscenter.sail.webapp.presentation.web.controllers.IndexController" />

	<bean id="selectteamController"
		class="org.telscenter.sail.webapp.presentation.web.controllers.student.SelectTeamController">
	    <property name="runService" ref="runService" />
	</bean>
	
	<bean id="startProjectController"
	    class="org.telscenter.sail.webapp.presentation.web.controllers.student.StartProjectController">
	    <property name="runService" ref="runService" />
	    <property name="workgroupService" ref="workgroupService" />
		<property name="httpRestTransport" ref="httpRestTransport" />
	</bean>
	
	<bean id="teamSignInController"
	    class="org.telscenter.sail.webapp.presentation.web.controllers.student.TeamSignInController">
	    <property name="commandName" value="teamSignInForm" />
	    <property name="commandClass" 
	        value="org.telscenter.sail.webapp.presentation.web.TeamSignInForm" />
	    <property name="runService" ref="runService" />
	    <property name="workgroupService" ref="workgroupService" />
	    <property name="userService" ref="userService" />
		<property name="httpRestTransport" ref="httpRestTransport" />
	    <property name="validator" ref="teamSignInFormValidator" />
    	<property name="formView" value="student/teamsignin" />
    	<property name="successView" value="student/teamsigninsuccess" />
	</bean>
	
	<bean id="addProjectController"
    	class="org.telscenter.sail.webapp.presentation.web.controllers.student.AddProjectController">
    	<property name="runService" ref="runService" />
    	<property name="groupService" ref="groupService" />
    	<property name="aclService" ref="aclServiceImpl" />
    	<property name="commandName" value="addProjectParameters" />    	
    	<property name="commandClass" 
    	    value="org.telscenter.sail.webapp.domain.impl.AddProjectParameters" />
    	<property name="validator" ref="addprojectparametersValidator" />
    	<property name="formView" value="student/addproject" />
    	<property name="successView" value="student/addprojectsuccess" />
    </bean>

	<bean id="studentIndexController" 
	    class="org.telscenter.sail.webapp.presentation.web.controllers.student.StudentIndexController">
	    <property name="runService" ref="runService" />
		<property name="httpRestTransport" ref="httpRestTransport" />
	</bean>

	<bean id="createRunController"
		class="org.telscenter.sail.webapp.presentation.web.controllers.CreateRunController">
		<property name="runService" ref="runService" />
		<property name="projectService" ref="projectService" />
		<property name="commandName" value="runParameters" />
		<property name="commandClass"
			value="org.telscenter.sail.webapp.domain.impl.RunParameters" />
	</bean>
	
	<bean id="myProjectRunsController"
	    class="org.telscenter.sail.webapp.presentation.web.controllers.teacher.run.RunListController">
		<property name="runService" ref="runService" />
		<property name="workgroupService" ref="workgroupService" />
		<property name="httpRestTransport" ref="httpRestTransport" />
	</bean>

